config_version=3

[application]

config/name="godot-smart-textures"
run/main_scene="res://platform2d-example/smart_textures_test.tscn"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "platform2d" )

[importer_defaults]

texture={
"flags/filter": true,
"flags/mipmaps": true
}
